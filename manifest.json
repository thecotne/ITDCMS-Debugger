{
	"name": "ITDCMS Debugger 2",
	"short_name": "ITDCMS_Debugger 2",
	"description": "ITDCMS Debugger is chrome extension for easy Debugging of ITDCMS",
	"version": "0.3.1",
	"icons": {
		"128": "icons/icon128.png",
		"16": "icons/icon16.png",
		"48": "icons/icon48.png"
	},
    "page_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "Open Debugger in new Tab"
    },
	"devtools_page": "devback.html",
	"manifest_version": 2,
	"background": {
		"page": "background.html"
	},
	"content_scripts": [{
		"all_frames": true,
		"matches": ["<all_urls>"],
		"js": ["contentscript.js"]
	}],
	"content_security_policy": "default-src 'self'  chrome-extension-resource: ; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval';  connect-src *; frame-src *;",
	"web_accessible_resources": [
		"*"
	],
	"permissions": ["storage", "tabs", "debugger", "http://*/*", "https://*/*", "contextMenus"]
}
